@page
@model SamplingPrototype.Pages.DatabaseModel
@{
    ViewData["Title"] = "Database";
}

<h2>Database</h2>

@Model.CollectionNames

<br />
<br />
<br />

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Integer</th>
            <th>String</th>
        </tr>

    </thead>

    <tbody>

        @foreach (MongoData item in Model.Collection)
        {
        <tr>
            <td>@item._id</td>
            <td>@item.someInteger</td>
            <td>@item.someString</td>
        </tr>
        }

    </tbody>

</table>

